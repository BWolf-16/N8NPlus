{
  "name": "N8NPlus",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "setup": "node src/setup-manager.js full",
    "setup-check": "node src/setup-manager.js check",
    "setup-unix": "chmod +x setup.sh && ./setup.sh",
    "setup-win": "setup.bat",
    "prestart": "node src/setup-manager.js check",
    "start": "concurrently \"npm run backend\" \"npm run react-start\" \"npm run wait-and-electron\"",
    "backend": "cd backend && npm start",
    "react-start": "cd frontend && npm start",
    "wait-and-electron": "wait-on http://localhost:3000 && wait-on http://localhost:9999 && electron ."
  },
  "dependencies": {
    "electron": "^27.0.0",
    "dockerode": "^4.0.7"
  },
  "devDependencies": {
    "concurrently": "^8.2.0",
    "wait-on": "^7.0.1"
  }
}